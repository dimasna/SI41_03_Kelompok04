<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TransaksiDetailTopUp">

    <TextView
        android:id="@+id/headerUserCoinText"
        android:layout_width="401dp"
        android:layout_height="129dp"
        android:layout_weight="1"
        android:gravity="center"
        android:paddingStart="16dp"
        android:text="Detail Total Top Up"
        android:textAlignment="textStart"
        android:textColor="#070101"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="RtlCompat" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343">

        <TextView
            android:id="@+id/title_balance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Top Up"
            android:textAlignment="center"
            android:textColor="#9E9696"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/nominal_total_bal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="8dp"
            android:text="130"
            android:textAlignment="center"
            android:textColor="#8871F3"
            android:textSize="40dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/currency_total_bal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Top Up"
            android:textAlignment="center"
            android:textColor="#9E9696"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.776"
        tools:layout_editor_absoluteX="16dp">

        <TextView
            android:id="@+id/title_taxes"
            android:layout_width="168dp"
            android:layout_height="wrap_content"
            android:text="Samantha R"
            android:textColor="#646161"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/currency_total_taxes"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:text="12 Maret 2020"
            android:textColor="#5E5959"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.901">

        <TextView
            android:id="@+id/title_users_coins"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Samantha R"
            android:textColor="#646161"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/currency_coins"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="12 Maret 2020"
            android:textColor="#5E5959"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65">

        <TextView
            android:id="@+id/title_users_tasks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Dimas N AL"
            android:textColor="#646161"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/currency_total_coins"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="12 Maret 2020"
            android:textColor="#5E5959"
            android:textSize="12sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/balance_detail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="Daftar Top Up"
        android:textColor="#9F000000"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.554"></TextView>
</androidx.constraintlayout.widget.ConstraintLayout>